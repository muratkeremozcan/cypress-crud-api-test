{
  "name": "cypress-crud-api-test",
  "version": "1.0.0",
  "description": "crud testing a serverless application with Cypress api tests",
  "scripts": {
    "typecheck": "tsc --skipLibCheck --noEmit",
    "lint": "eslint --ext=.js,.ts,.tsx",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|json)\"",
    "fix-format": "yarn prettier -- --write",
    "check-format": "yarn prettier -- --list-different",
    "validate": "npm-run-all --parallel typecheck lint check-format",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "author": "Murat Ozcan",
  "license": "ISC",
  "devDependencies": {
    "@bahmutov/cy-api": "2.2.1",
    "@types/faker": "6.6.8",
    "@types/node": "18.11.11",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "@withshepherd/faker": "5.5.5",
    "cy-spok": "1.5.2",
    "cypress": "11.0.",
    "cypress-data-session": "2.3.1",
    "cypress-each": "1.13.1",
    "cypress-plugin-api": "2.6.1",
    "eslint": "8.29.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-no-only-tests": "3.1.0",
    "husky": "4.3.8",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.0",
    "typescript": "4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck && lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "eslint"
    ],
    "**/*.+(js|ts|json)": [
      "prettier --write"
    ]
  }
}
