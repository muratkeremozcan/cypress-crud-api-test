{
  "name": "cypress-crud-api-test",
  "version": "1.0.0",
  "description": "crud testing a serverless application with Cypress api tests",
  "scripts": {
    "typecheck": "tsc --skipLibCheck --noEmit",
    "lint": "eslint --ext=.js,.ts,.tsx",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "author": "Murat Ozcan",
  "license": "ISC",
  "devDependencies": {
    "@bahmutov/cy-api": "1.6.2",
    "@types/faker": "5.5.9",
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "@withshepherd/faker": "5.5.5",
    "cy-spok": "1.4.1",
    "cypress": "9.2.1",
    "cypress-data-session": "1.15.1",
    "cypress-each": "1.10.0",
    "eslint": "8.7.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-no-only-tests": "2.6.0",
    "husky": "4.3.5",
    "jsonwebtoken": "8.5.1",
    "typescript": "4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn typecheck"
    }
  }
}
